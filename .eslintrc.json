{
    "extends": "eslint:recommended",
    "env": {
        "browser": true,
        "es6": true,
        "jasmine": true
    },
    "parserOptions": {
        "ecmaVersion": 6,
        "sourceType": "module"
    },
    "rules": {
        "no-unused-vars": ["error", { "vars": "all", "args": "none" }]
    }
}
